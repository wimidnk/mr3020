
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option txpower '15'
	option country 'US'
	option disabled '0'
	option channel '10'

config wifi-iface
	option ssid 'AXIOM'
	option encryption 'psk2'
	option device 'radio0'
	option mode 'sta'
	option key 'bismillah'
	option network 'wwan'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'WER'
	option encryption 'psk2'
	option key 'bismillah'
	option network 'lan'

